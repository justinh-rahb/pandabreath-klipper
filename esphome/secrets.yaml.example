# ESPHome secrets — DO NOT COMMIT with real values.
# This file is gitignored. Copy it and fill in your actual credentials.

# WiFi
wifi_ssid: "YourWiFiSSID"
wifi_password: "YourWiFiPassword"
wifi_ap_password: "panda-breath-setup"

# MQTT broker — must be reachable from the Panda Breath on your LAN.
# The Klipper module on the Snapmaker U1 also connects to this broker.
# A local Mosquitto instance on the U1 or a LAN Pi works well.
mqtt_broker: "192.168.1.x"
mqtt_username: ""
mqtt_password: ""

# ESPHome native API (used by Home Assistant; can leave as random string if not using HA).
# Generate with: python3 -c "import base64,os; print(base64.b64encode(os.urandom(32)).decode())"
api_encryption_key: "REPLACE_WITH_32_BYTE_BASE64_KEY"

# OTA update password
ota_password: "REPLACE_WITH_SECURE_PASSWORD"
